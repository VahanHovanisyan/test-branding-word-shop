{
	"reset-css": {
		"scope": "css,scss",
		"prefix": "reset",
		"body": [
			"*,*:before,*:after{box-sizing:inherit}",
			"html{box-sizing:border-box}"
		],
		"description": "обнуление стилей по бем"
	},
	"magic transition": {
		"scope": "css,scss",
		"prefix": "tr",
		"body": "transition: all 0.3s ease 0s;",
		"description": "transition"
	},
	"magic animation": {
		"scope": "css,scss",
		"prefix": "a",
		"body": "animation: name 1s ease infinite alternate 0s forwards;",
		"description": "animation"
	},
	"transform-universal": {
		"scope": "css,scss",
		"prefix": "t",
		"body": "transform: translate(0, 0) scale(1, 1) rotate( 0deg) skew(0deg, 0deg);",
		"description": "transform-universal"
	},
	"margin auto": {
		"scope": "css,scss",
		"prefix": "m",
		"body": "margin: 0px auto;",
		"description": "margin auto"
	},
	"media 1440px": {
		"scope": "css,scss",
		"prefix": "7",
		"body": "@media(max-width:1440px){$1}",
		"description": "media 1440px"
	},
	"media 1199.98px": {
		"scope": "css,scss",
		"prefix": "6",
		"body": "@media(max-width:1199.98px){$1}",
		"description": "media 1199.98px"
	},
	"media 1024px": {
		"scope": "css,scss",
		"prefix": "5",
		"body": "@media(max-width:1024px){$1}",
		"description": "media 1024px"
	},
	"media 991.98px": {
		"scope": "css,scss",
		"prefix": "4",
		"body": "@media(max-width:991.98px){$1}",
		"description": "media 991.98px"
	},
	"media 767.98px": {
		"scope": "css,scss",
		"prefix": "3",
		"body": "@media(max-width:767.98px){$1}",
		"description": "media 767.98px"
	},
	"media 576.98px": {
		"scope": "css,scss",
		"prefix": "2",
		"body": "@media(max-width:576.98px){$1}",
		"description": "media 576.98px"
	},
	"media 479.98px": {
		"scope": "css,scss",
		"prefix": "1",
		"body": "@media(max-width:479.98px){$1}",
		"description": "media 479.98px"
	},
	"внешний отступ": {
		"scope": "css,scss",
		"prefix": "mm",
		"body": [
			"margin: 0px 0px 0px 0px;"
		],
		"description": "внешний отступ"
	},
	"внутрений отступ": {
		"scope": "css,scss",
		"prefix": "pp",
		"body": [
			"padding: 0px 0px 0px 0px;"
		],
		"description": "внутрений отступ"
	},
	"adaptive calc": {
		"scope": "css,scss",
		"prefix": "c",
		"body": [
			"calc($1px + ($2 - $3) * ((100vw - 320px) / ($4 - 320)));"
		],
		"description": "adaptive calc"
	},
	"adaptive all mixin": {
		"scope": "css,scss",
		"prefix": "all",
		"body": [
			"@include media-adaptiv-all(property,max,min);"
		],
		"description": "adaptive all property mixin"
	},
	"adaptive font mixin": {
		"scope": "css,scss",
		"prefix": "font",
		"body": [
			"@include media-adaptiv-font(max,min);"
		],
		"description": "adaptive font-size mixin"
	}
}
